#reader(lib "htdp-beginner-reader.ss" "lang")((modname pairs_and_lists) (read-case-sensitive #t) (teachpacks ()) (htdp-settings #(#t constructor repeating-decimal #f #t none #f () #f)))

(define myList (cons "a"  (cons "b" (cons "c" null))))

(cons "a" '())

(first myList) ; First element of the list/pair
(rest myList) ; The rest of the pair chain except the first element
(list-ref myList 1)

(length myList)


